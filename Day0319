public class Day0319
{
  public static void main(String args[])
  {
    Person person2=new Person();
    System.out.println(person2.age);
    person2.change(person2);
    System.out.println(person2.age);
    
  }
    
    
    
}

class Person
{
  int age=20;
  
  public void change(Person person)
  {
  person.age=++person.age;
  }
}
